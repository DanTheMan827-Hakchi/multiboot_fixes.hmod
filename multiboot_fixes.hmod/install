source "$transferpath/b7200_multiboot"

cfg_multiboot_boardtype="$(cat "$mountpoint/etc/clover/boardtype")"
cfg_multiboot_region="$(cat "$mountpoint/etc/clover/REGION")"
cfg_multiboot_hardware_id="$(multiboot_getcode)"
cfg_firmware="_nand_"

[ -f "$installpath/menu" ] && rm "$installpath/menu"

copy "$transferpath/b7200_multiboot" "$preinitpath/"
copy "$transferpath/p7200_multiboot" "$preinitpath/"
copy "$transferpath/hsqs_expand_path" "$rootfs/bin/"

chmod +x "$rootfs/bin/hsqs_expand_path"

local realgamespath="$installpath/games/$cfg_multiboot_hardware_id"

if [ ! -d "$rootfs/usr/share/games" ]; then
  mkdir -p "$rootfs/usr/share/games"
fi

if [ ! -d "$realgamespath" ]; then
  mkdir -p "$(dirname $realgamespath)"
  mv "$rootfs/usr/share/games" "$realgamespath"
  mkdir -p "$rootfs/usr/share/games"
fi

mkdir -p "$installpath/games/nes-usa"
mkdir -p "$installpath/games/nes-jpn"
mkdir -p "$installpath/games/snes-usa"
mkdir -p "$installpath/games/snes-jpn"

return 1
